{
  "name": "uber-mcp-pampas",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:all": "concurrently \"npm run server\" \"npm run client\" \"npm run mcp:inspect\"",
    "client": "vite",
    "server": "nodemon server/index.js",
    "mcp:inspect": "cd mcp-server && npx @modelcontextprotocol/inspector node index.js",
    "mcp:test": "cd mcp-server && npm test",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "vite": "^7.1.7"
  }
}
